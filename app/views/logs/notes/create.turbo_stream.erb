<%= turbo_stream.replace('ack_' + dom_id(@notable)) do %>
  <%= render AcknowledgeButtonComponent.new(
               notable: @notable,
               readonly: cannot?(:manage, @notable.loggable)
             )
   %>
<% end %>

<%= turbo_stream.replace('notes') do %>
  <%= turbo_frame_tag("notes", src: log_notes_path(@notable, active: 1),
                            refresh: "morph") %>
<% end %>

