<%= link_to connector_path(connector), class: 'list-group-item list-group-item-action' do %>
  <div>
    <span class="subtitle">
      <%= t('activerecord.models.connector') %>:
      #<%= connector.id %>
    </span>
  </div>
  <div  class="row">
    <div class="col-3"><%= connector.name %></div>
    <div class="col-3"><%= connector.ip %></div>
    <div class="col-6"><%= connector.condition_message %></div>
  </div>
  <div>
    <%= render TagListComponent.new(item: connector) %>
  </div>
<% end %>

